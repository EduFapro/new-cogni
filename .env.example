# Example environment file for Novo Cogni
# Copy this to .env and fill in real values before running the app.

ENV=development

# Must be exactly 32 characters
ENCRYPTION_KEY=CHANGE_THIS_TO_A_32_CHAR_SECRET_KEY

# Must be exactly 16 characters
ENCRYPTION_IV=CHANGE_THIS_TO_A_16_CHAR_IV

# Backend API Configuration
BACKEND_URL=http://localhost:8080
ENABLE_SYNC=true
API_KEY=your-api-key-here
